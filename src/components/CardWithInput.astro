---
interface Props {
	title: string;
	roomCode: string;
}
import "../styles.css";
const { roomCode, title, body } = Astro.props;
---

<li class="link-card flex items-center justify-center h-48 ">
	<a>
		<h2>
			{title}
			<span>&rarr;</span>
		</h2>
		<form>
		<label> Room Code: </label><input class="rounded-sm outline	" type="text" id="roomCode" size=4 name="roomCode" maxlength=4>
		<button>Go</button>
		</form>
	</a>
</li>

